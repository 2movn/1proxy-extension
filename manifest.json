{
    "manifest_version": 3,
    "name": "1Proxy Manager",
    "description": "Extension change ip for api 1proxy, proxy list",
    "version": "2.0",
    "permissions": [
        "proxy",
        "privacy",
        "tabs",
        "storage",
        "webRequest",
        "webRequestAuthProvider"
    ],
    "host_permissions": [
        "https://1proxy.net/*"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "__1PROXY__",
        "default_icon": {
            "16": "icons/16_16.png",
            "48": "icons/48_48.png",
            "128": "icons/128_128.png"
        }
    },
    "icons": {
        "16": "icons/16_16.png",
        "48": "icons/48_48.png",
        "128": "icons/128_128.png"
    },
    "minimum_chrome_version": "100.0.0"
}